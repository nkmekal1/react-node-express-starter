{
  "name": "react-node-express-starter",
  "version": "1.0.0",
  "description": "A boiler plate for creating apps using React, Node, Express, Webpack and Babel 7",
  "main": "app.js",
  "scripts": {
    "loc": "./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node app.js --ignore src/client"
  },
  "keywords": [
    "React",
    "Node",
    "Express",
    "Webpack",
    "Babel 7"
  ],
  "author": "MNK",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "express": "^4.16.4",
    "express-hbs": "^1.0.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.3"
  }
}
